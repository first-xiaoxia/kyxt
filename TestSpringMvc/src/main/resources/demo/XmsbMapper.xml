  </sql>

  <select id="selectXmsbByPrimaryKey" resultType="com.java.entity.kyxm.XmsbPo" parameterType="java.lang.String" >
    select 
    <include refid="Base_Column_List" />
    from t_kyxm
    where xmlsh = #{xmlsh}
